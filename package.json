{
  "name": "realm-studio",
  "productName": "Realm Studio",
  "version": "3.4.0",
  "description": "A tool for everything Realm!",
  "author": {
    "name": "Realm Inc.",
    "email": "info@realm.io",
    "url": "https://realm.io"
  },
  "repository": "https://github.com/realm/realm-studio",
  "license": "Apache-2.0",
  "main": "./build/main.bundle.js",
  "build": {
    "appId": "io.realm.realm-studio",
    "directories": {
      "buildResources": "./resources"
    },
    "files": [
      "./build/**/*",
      "./node_modules/**/*",
      "./package.json",
      "./static/**/*",
      "!node_modules/realm/android${/*}",
      "!node_modules/realm/build${/*}",
      "!node_modules/realm/react-native${/*}",
      "!node_modules/realm/scripts${/*}",
      "!node_modules/realm/src${/*}",
      "!node_modules/realm/vendor${/*}"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "dmg": {
      "iconSize": 90,
      "contents": [
        {
          "x": 95,
          "y": 200
        },
        {
          "x": 430,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "tar.gz"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable",
        "zip"
      ],
      "icon": "resources/icon.ico"
    },
    "nsis": {
      "installerIcon": "resources/icon.ico",
      "perMachine": true
    },
    "protocols": [
      {
        "name": "Realm Studio",
        "schemes": [
          "x-realm-cloud",
          "x-realm-studio"
        ]
      }
    ],
    "fileAssociations": {
      "ext": "realm",
      "name": "Realm",
      "isPackage": true
    },
    "publish": [
      {
        "provider": "s3",
        "bucket": "static.realm.io",
        "region": "us-east-1",
        "path": "downloads/realm-studio"
      }
    ],
    "npmArgs": [
      "--fallback-to-build=false"
    ]
  },
  "scripts": {
    "build": "webpack --mode production --config=configs/webpack.js",
    "check:package-lock": "node ./scripts/check-package-lock.js",
    "check:auto-update-files": "node ./scripts/check-auto-update-files.js",
    "dev:ros-https": "ros start --https --https-key ./data/keys/server.key --https-cert ./data/keys/server.crt",
    "dev:ros-unreliable": "node ./scripts/unreliable-ros.js",
    "dev:ros": "ros start",
    "dev": "rm -rf ./build && concurrently --names \"WEBPACK:M,WEBPACK:R,ELECTRON\" \"npm run webpack:main\" \"npm run webpack:renderer\" \"npm run wait-for-bundle && electron .\"",
    "fix-headers": "node ./scripts/fix-headers.js",
    "lint:sass": "sass-lint --max-warnings=0 -c .sass-lint.yml 'src/**/*.scss' -v",
    "lint:ts": "tslint --project tsconfig.json 'src/**/*.ts' 'src/**/*.tsx'",
    "lint": "npm run lint:ts && npm run lint:sass && echo 'Everything looks good!'",
    "package": "electron-builder --publish=never",
    "postinstall": "electron-builder install-app-deps",
    "prepackage": "rm -rf ./build && npm run build",
    "prestart": "rm -rf ./build && npm run build",
    "generate-https-certificate": "openssl req -x509 -newkey rsa:4096 -keyout data/keys/server.key -out data/keys/server.crt -days 365 -nodes",
    "generate-all-types-realm": "node ./scripts/generate-realm.js",
    "start": "electron .",
    "sentry:upload-symbols": "node ./scripts/sentry-symbols.js",
    "test": "mocha-webpack --opts=configs/mocha-webpack.opts",
    "test:post-package": "mocha-webpack --opts=configs/mocha-webpack.opts src/testing/post-packaging/index.ts",
    "tslint-prettier": "tslint --fix 'src/**/*.ts' 'src/**/*.tsx'",
    "wait-for-bundle": "node ./scripts/wait-for-bundle.js",
    "webpack:main": "webpack --mode development --config=configs/webpack.main.js",
    "webpack:renderer": "webpack-dev-server --mode development --config=configs/webpack.renderer.js"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "npm run fix-headers --"
    ],
    "*.js": [
      "npm run fix-headers --"
    ],
    "*.scss": [
      "npm run fix-headers --"
    ],
    "package.json": [
      "npm run check:package-lock --"
    ]
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^13.0.0",
    "@contentful/rich-text-types": "^13.0.0",
    "@sentry/electron": "^0.15.0",
    "classnames": "^2.2.5",
    "contentful": "^7.3.0",
    "electron-store": "^2.0.0",
    "electron-updater": "4.0.6",
    "font-awesome": "^4.7.0",
    "fs-extra": "^7.0.1",
    "isomorphic-fetch": "^2.2.1",
    "json5": "^2.1.0",
    "jwt-decode": "^2.2.0",
    "keytar": "^4.4.1",
    "memoize-one": "^5.0.0",
    "mixpanel-browser": "^2.26.0",
    "moment": "^2.24.0",
    "papaparse": "^4.6.3",
    "raven": "^2.6.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-draggable": "^3.1.1",
    "react-object-inspector": "^0.2.1",
    "react-realm-context": "^0.1.0",
    "react-sortable-hoc": "^1.6.1",
    "react-virtualized": "^9.20.1",
    "reactstrap": "^7.1.0",
    "realm": "2.23.0",
    "semver": "^5.4.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@octokit/rest": "^16.16.0",
    "@sentry/cli": "^1.39.1",
    "@sentry/webpack-plugin": "^1.6.2",
    "@types/classnames": "^2.2.7",
    "@types/electron-store": "^1.3.0",
    "@types/faker": "^4.1.5",
    "@types/fs-extra": "^5.0.5",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jsdom": "^12.2.2",
    "@types/json5": "0.0.30",
    "@types/jwt-decode": "^2.2.1",
    "@types/keytar": "^4.0.1",
    "@types/mixpanel": "^2.14.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.4",
    "@types/papaparse": "^4.5.7",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-sortable-hoc": "^0.6.5",
    "@types/react-virtualized": "^9.18.12",
    "@types/reactstrap": "^7.1.3",
    "@types/semver": "^5.5.0",
    "@types/source-map-support": "^0.4.2",
    "@types/tmp": "0.0.34",
    "@types/uuid": "^3.4.2",
    "@types/webdriverio": "^4.13.3",
    "@types/webpack": "^4.4.24",
    "@types/webpack-env": "^1.13.7",
    "awesome-typescript-loader": "^v5.2.1",
    "bootstrap": "^4.3.1",
    "commander": "^2.19.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^2.1.0",
    "devtron": "^1.4.0",
    "electron": "3.0.15",
    "electron-builder": "20.38.5",
    "electron-download": "^4.1.1",
    "electron-publisher-s3": "^20.17.2",
    "faker": "^4.1.0",
    "file-loader": "^3.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^1.3.1",
    "js-yaml": "^3.12.1",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.1.4",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.15.0",
    "mocha-webpack": "2.0.0-beta.0",
    "node-sass": "^4.11.0",
    "null-loader": "^0.1.1",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.7.1",
    "remark": "^10.0.1",
    "resolve-url-loader": "^3.0.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "simple-git": "^1.80.1",
    "source-map-support": "^0.5.10",
    "spectron": "^5.0.0",
    "spectron-fake-dialog": "0.0.1",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.3",
    "tmp": "0.0.33",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "why-did-you-update": "^1.0.6",
    "xvfb-maybe": "^0.2.1"
  },
  "engines": {
    "node": "^8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
