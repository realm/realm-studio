{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",

    "allowJs": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es5",
      "es6"
    ],
    "noImplicitAny": true,
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "build",
    "dist"
  ]
}
